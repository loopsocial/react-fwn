{
  "env": {
    "es6": true,
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "import/default": 0,
    "react/display-name": 0,
    "react/prop-types": 0,
    "prettier/prettier": 2,
    "semi": [2, "never"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js"]
      }
    }
  }
}
